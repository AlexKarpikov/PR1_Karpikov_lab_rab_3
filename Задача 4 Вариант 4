<script> 
$(document).ready(function () 
 { 
   $("button").click(function () 
   { 
   $('#div1').delay(100).animate({marginTop:+35},"slow");
   $('#div3').delay(100).animate({marginTop:+35},"slow");
   $('#div5').delay(100).animate({marginTop:+35},"slow");
   });
 }); 
</script> 
</head> 
